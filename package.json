{
    "name": "1lend-io",
    "version": "0.0.1",
    "scripts": {
        "ng": "ng",
        "start": "cd web && ng serve",
        "build": "cd web && ng build --prod --build-optimizer --aot=true",
        "test": "cd web && ng test",
        "lint": "cd web && ng lint",
        "e2e": "cd web && ng e2e",
        "postinstall": "node patch.js",
        "sm:test": "scripts/test.sh",
        "sm:compile": "etherlime compile --runs 200",
        "sm:deploy:dev": "etherlime compile && etherlime deploy",
        "presm:deploy:prod": "yarn run sm:compile",
        "sm:deploy:prod": "etherlime deploy --compile false --file ./deployment/deploy-mainnet.js",
        "presm:deploy:getBestPrice:prod": "yarn run sm:compile"
    },
